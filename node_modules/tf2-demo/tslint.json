{
  "extends": "tslint:recommended",
  "rules": {
    "max-line-length": {
      "options": [
        140
      ]
    },
    "object-literal-sort-keys": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "indent": [
      true,
      "tabs"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": false,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": true,
    "no-console": {
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        140
      ]
    }
  }
}
